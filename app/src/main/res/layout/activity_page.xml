<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PageActivity">

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/button_format5"
        android:orientation="horizontal"
        android:padding="30px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.326"
        app:layout_constraintStart_toEndOf="@+id/linearLayout2"
        app:layout_constraintTop_toBottomOf="@id/linearLayout"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/textView14"
            android:layout_width="70dp"
            android:layout_height="33dp"
            android:gravity="start|center"
            android:text="RESTE"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@color/colorgris" />

        <ImageView
            android:id="@+id/Img14"
            android:layout_width="60dp"
            android:layout_height="70dp"
            android:layout_gravity="fill_horizontal"
            android:src="@drawable/asset_5" />

        <TextView
            android:id="@+id/reste1"
            android:layout_width="90dp"
            android:layout_height="40dp"
            android:hint="xxx.xxx DH"
            android:inputType="text"
            android:translationX="-130dp"
            android:translationY="33dp"
            tools:layout_editor_absoluteX="168dp"
            tools:layout_editor_absoluteY="243dp" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="168dp"
        android:layout_marginRight="168dp"
        android:text="@string/my_money_controller"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/colorPrimaryDark"
        app:layout_constraintBottom_toBottomOf="@id/image5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/image5"
        app:layout_constraintVertical_bias="0.517"
        tools:ignore="HardcodedText,MissingConstraints" />

    <ImageView
        android:id="@+id/image5"
        android:layout_width="75dp"
        android:layout_height="53dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.008"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/asset2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="38dp" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="350dp"
        android:layout_height="80dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/button_format3"
        android:orientation="horizontal"
        android:padding="30px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/textView12"
            android:layout_width="219dp"
            android:layout_height="33dp"
            android:gravity="top|center"
            android:text="SOMME"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@color/colorgris" />

        <ImageView
            android:id="@+id/Img12"
            android:layout_width="90dp"
            android:layout_height="80dp"
            android:layout_gravity="fill_horizontal"
            android:src="@drawable/asset_4" />

        <TextView
            android:id="@+id/somme1"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:hint="xxxx.xxx DH"
            android:inputType="text"
            android:translationX="-250dp"
            android:translationY="33dp"
            tools:layout_editor_absoluteX="168dp"
            tools:layout_editor_absoluteY="243dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="180dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/buttun_format4"
        android:orientation="horizontal"
        android:padding="30px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/linearLayout"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/textView13"
            android:layout_width="100dp"
            android:layout_height="33dp"
            android:gravity="start|center"
            android:text="DEPENSES"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@color/colorgris" />

        <ImageView
            android:id="@+id/Img13"
            android:layout_width="80dp"
            android:layout_height="70dp"
            android:layout_gravity="fill_horizontal"
            android:src="@drawable/asset_3" />

        <TextView
            android:id="@+id/depense1"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:hint="xxx.xxx DH"
            android:inputType="text"
            android:translationX="-180dp"
            android:translationY="33dp"
            tools:layout_editor_absoluteX="168dp"
            tools:layout_editor_absoluteY="243dp" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Les dÃ©penses du mois"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/colorPrimaryDark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.12"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/linearLayout2"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/ajout"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/group_29"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/progressBar"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/ImageView1"
        android:layout_width="fill_parent"
        android:layout_height="80dp"
        android:translationY="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:srcCompat="@drawable/button_format"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-30dp" />

    <Button
        android:id="@+id/imageView2"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginEnd="68dp"
        android:layout_marginRight="68dp"
        android:background="@drawable/icon_ionic_ios_home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageButton3"
        app:layout_constraintHorizontal_bias="0.838"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ImageView1"
        app:layout_constraintVertical_bias="0.84"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/imageButton3"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="156dp"
        android:layout_marginLeft="156dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/icon_awesome_chart_pie"
        app:layout_constraintStart_toStartOf="@+id/ImageView1"
        app:layout_constraintTop_toTopOf="@+id/ImageView1"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/imageButton4"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="64dp"
        android:layout_marginLeft="64dp"
        android:background="@drawable/icon_material_settings"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageButton3"
        app:layout_constraintTop_toTopOf="@+id/ImageView1"
        app:layout_constraintVertical_bias="0.777"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="108dp"
        android:text="Les reste du mois"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/colorPrimaryDark"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/reste2"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_marginTop="108dp"
        android:gravity="center_horizontal|center_vertical"
        android:hint="xxxx DH"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.833"
        app:layout_constraintStart_toEndOf="@id/textView16"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        tools:ignore="MissingConstraints" />

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="330dp"
        android:layout_height="33dp"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <Button
        android:id="@+id/out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="sign out"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/buttonVoir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Voir"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.529"
        app:layout_constraintStart_toEndOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />


</androidx.constraintlayout.widget.ConstraintLayout>